<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/decorators">
	<excludes>
		<pattern>/nodecorate/*</pattern>
		<pattern>*.css</pattern>
		<pattern>/scripts/*</pattern>
		<pattern>/images/*</pattern>
		<pattern>/*.htm</pattern>
		<pattern>/*.html</pattern>
	</excludes>
	<decorator name="frontlayout" page="layout.jsp">
		<pattern>*/outer/*</pattern>
	</decorator>
	<decorator name="authenticatedlayout" page="InnerLayout.jsp">
		<pattern>*/authenticated/*</pattern>
	</decorator>
</decorators>   